{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "Output": "",
    "blocks": [{
        "alignment": "left",
        "segments": [{
                "type": "session",
                "style": "plain",
                "foreground": "#c386f1",
                "leading_diamond": "\uE0B6",
                "trailing_diamond": "\uE0B0",
                "properties": {
                    "template": "{{ if .SSHSession }}\uF817 {{ end }}"
                }
            },
            {
                "type": "python",
                "style": "plain",
                "powerline_symbol": "\uE0B0",
                "foreground": "#16c60c",
                "properties": {
                    "template": "\uE235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }} {{ .Full }} {{ end }}"
                }
            },
            {
                "foreground": "#ffffff",
                "properties": {
                    "template": "<#C591E8>\u276f</><#69FF94>\u276f</>"
                },
                "style": "plain",
                "type": "text"
            },
            {
                "foreground": "#56B6C2",
                "properties": {
                    "style": "folder",
                    "template": " {{ .Path }} "
                },
                "style": "plain",
                "type": "path"
            },
            {
                "foreground": "#D0666F",
                "properties": {
                    "branch_icon": "",
                    "fetch_stash_count": true,
                    "fetch_status": true,
                    "fetch_upstream_icon": true,
                    "template": "<#5FAAE8>git:(</>{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}\uf044{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }}<#5FAAE8>) </>"
                },
                "style": "plain",
                "type": "git"
            },
            {
                "foreground": "#DCB977",
                "foreground_templates": [
                    "{{ if gt .Code 0 }}#e91e63{{ end }}"
                ],
                "properties": {
                    "always_enabled": true,
                    "template": "{{ if gt .Code 0 }}ðŸ”´{{ else }}ðŸŸ¢{{ end }} "
                },
                "style": "plain",
                "type": "exit"
            },
            {
                "type": "root",
                "style": "powerline",
                "foreground": "#ffff66",
                "properties": {
                    "template": "\uF0E7 "
                }
            }
        ],
        "type": "prompt"
    }],
    "version": 1
}
